@model IEnumerable<TennisClubDTO>
<div class="col-md-9">
    @foreach (var club in Model)
    {
    <div class="col-sm-4 col-lg-4 col-md-4">
        <div class="thumbnail">
            <img src="@club.Image" alt="">
            <div class="caption">
                <h3 class="pull-right">@club.Schedule</h3>
                <h3>
                    <a asp-controller=""
                       asp-action=""
                       >@club.Name</a>
                </h3>
                <p>@club.Description</p>
            </div>
            <div class="addToCart">
                <p class="button">
                    <a class="btn btn-primary"
                       asp-controller="ShoppingCart"
                       asp-action="AddToShoppingCart"
                       >Book</a>
                </p>
            </div>
        </div>
    </div>
    }
    
</div>